{{> asset-download .}}
<div class="table-responsive">
    <div class="col-lg-12">
        <div id='swagger-viewer'>
            <div class="col-lg-2">
                <b>
                    {{t "Raw Content :"}}
                </b>
            </div>
            {{#if content}}
                <textarea id="swaggercontent" name="swaggercontent" style="width:100%" style="width:100%">{{../content}}</textarea>
            {{else}}
                <p>{{t "No Swagger Content To Display"}}</p>
            {{/if}}
        </div>
    </div>
</div>