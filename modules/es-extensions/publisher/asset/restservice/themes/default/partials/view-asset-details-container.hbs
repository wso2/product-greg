{{#with assets}}
{{#each tables}}
{{renderTablePreview .}}
{{/each}}

{{#if isDependentsPresent}}

<h2 class="field-title">
 <a class="collapsing-h2 collapsed" data-toggle="collapse" area-expanded="true" href="#collapseassociations">
    <i class="cu-btn-exp-col btn-collapsed">{{t "Associations"}}</i>
</a>
</h2>
<div id="collapseassociations" class="collapse">
    <table class="table table-bordered">
        <thead class="decorated-table-heading">
            <tr><th>
                {{t "Associated Artifacts"}}
            </th>
                <th>
                    {{t "Version"}}
                </th>
        </tr>
    </thead>
    <tbody>
        {{#each dependencies}}
        <tr>
            <td><a href="../../../assets/{{this.associationType}}/details/{{this.associationUUID}}">{{this.associationName}}</a></td>
            <td>{{#if this.associationVersion}} {{this.associationVersion}} {{/if}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
</div>

{{/if}}
{{/with}}