{{#if uniqueColor}}
    <div class="ast-img setbgcolor" style="background: {{uniqueColor}}">
    <span class="ast-type-icon">
      <i class="fw fw-wadl fw-lg"></i>
      </span>
        <div class="ast-name-icon">{{nameToChar}}</div>
    </div>
{{else}}
    <div class="ast-img setbgcolor" style="background: {{assets.uniqueColor}}">
    <span class="ast-type-icon">
      <i class="fw fw-wadl fw-lg"></i>
      </span>
        <div class="ast-name-icon">{{assets.nameToChar}}</div>
    </div>
{{/if}}